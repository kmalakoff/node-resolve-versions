{
  "name": "node-resolve-versions",
  "version": "1.0.0",
  "description": "Convert a version expression to released Node.js versions (by full or partial semver, expression, or package.json engines.node)",
  "keywords": [
    "node",
    "semver",
    "version",
    "versions",
    "string",
    "expression",
    "package.json",
    "engines",
    "engines.node"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-resolve-versions.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "isarray": "^2.0.5",
    "lodash.uniq": "^4.5.0",
    "node-semvers": "^1.2.2",
    "semver": "^5.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "depcheck": "^1.4.7",
    "ts-dev-stack": "^1.5.3"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
