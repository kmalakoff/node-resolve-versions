{
  "name": "node-resolve-versions",
  "version": "0.3.2",
  "description": "Convert a version expression to released Node.js versions (by full or partial semver, expression, or package.json engines.node)",
  "keywords": [
    "node",
    "semver",
    "version",
    "versions",
    "string",
    "expression",
    "package.json",
    "engines",
    "engines.node"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-resolve-versions.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && depcheck",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts",
    "test:engines": "nvu engines npm test"
  },
  "dependencies": {
    "isarray": "^2.0.5",
    "lodash.uniq": "^4.5.0",
    "node-semvers": "^0.6.2",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
