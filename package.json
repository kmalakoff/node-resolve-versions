{
  "name": "node-resolve-versions",
  "version": "0.3.10",
  "description": "Convert a version expression to released Node.js versions (by full or partial semver, expression, or package.json engines.node)",
  "keywords": [
    "node",
    "semver",
    "version",
    "versions",
    "string",
    "expression",
    "package.json",
    "engines",
    "engines.node"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-resolve-versions.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds test:node --timeout=40000",
    "test:engines": "nvu engines tsds test:node --timeout=40000",
    "version": "tsds version"
  },
  "dependencies": {
    "isarray": "^2.0.5",
    "lodash.uniq": "^4.5.0",
    "node-semvers": "^0.6.11",
    "semver": "^5.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.17",
    "depcheck": "^1.4.7",
    "ts-dev-stack": "^0.9.10"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.js",
    "targets": [
      "cjs"
    ]
  }
}
